{
	"name": "treqs",
	"displayName": "TReqs",
	"description": "TReqs: Tool Support for Managing Requirements in Large-Scale Agile System Development",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/piotr20a/vscode-treqs"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:treqs.helloWorld",
		"onCommand:treqs.testCommand"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "treqs.helloWorld",
				"category": "TReqs",
				"title": "Hello World"
			},
			{
				"command": "treqs.testCommand",
				"category": "TReqs",
				"title": "Treqs List"
			}
		],
		"snippets": [
			{
				"path": "./src/treqs-global.code-snippets"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceScheme == file",
					"command": "treqs.testCommand",
					"alt": "treqs.helloWorld",
					"group": "z_commands"
				}
			]
		}
	},
	"scripts": {
		"python": "npm install python-shell",
		"python-treqs": "python pip install -e ..\\..\\..\\Desktop\\treqs\\",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.17.3",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"child_process": "^1.0.2",
		"python-shell": "^3.0.0"
	}
}
